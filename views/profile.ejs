<div class="container-profile">

  <div class="profile-header">
    <div class="overlay">
      <div class="header-top">
        <img class="profile-pic" src="images/tennis_dude.jpg">
          <% if (user) { %>
          <h1 class="profile-title"><%= user.name %></h1>
          <% } %>
        <% include ./_racquetForm %>
      </div>
    </div>
  </div>

  <div class="media">
    <div class="media-left"><img src="" alt=""></div>
      <div class="media-body">
        <h3>My Racquets</h3>
        <ul class="collection">

          <% for (var racquetss of user.racquets) { %>
            <% for (var racquets of racquetss) { %>
              <li class="collection-item avatar">
                <img src="images/tennis_dude.jpg" alt="" class="circle">
                <span class="title"><%= racquets.owner %></span>
                <% for (var inventoryItems of racquets.inventoryItems) { %>
                  <p><%= inventoryItems %></p>
                <% } %>
                <p><%= racquets.tension %></p>
                <a href="#!" class="secondary-content"><i class="material-icons">more</i></a>
              </li>

            <% } %>
          <% } %>
        </ul>
      </div>
      <hr></hr>
  </div>

</div>
