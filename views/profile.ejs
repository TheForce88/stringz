<div class="container-profile">

  <div class="profile-header">
    <div class="overlay">
      <div class="header-top">
        <img class="profile-pic" src="images/tennis_dude.jpg">
          <% if (user) { %>
          <h1 class="profile-title"><%= user.name %></h1>
          <% } %>
        <% include ./_racquetForm %>
      </div>
    </div>
  </div>

  <div class="media">
    <div class="media-left"><img src="" alt=""></div>
      <div class="media-body">
        <h3>Recent Stringz</h3>
        <ul class="collection">

          <% for (let racquet of user.racquets) { %>
            <li class="collection-item avatar" data-id="<%= racquet._id %>">
              <img src="images/tennis_dude.jpg" alt="" class="circle">
              <a href="/api/racquets/<%= racquet._id %>" class="label label-default"><%= racquet.owner %></a>
              <span class="title">inventory items: <%= racquet.inventoryItems %></span>
              <span class="title">tension: <%= racquet.tension %></span>
            </li>
          <% } %>

          <li class="collection-item avatar">
            <img src="images/tennis_dude.jpg" alt="" class="circle">
            <span class="title">Title</span>
            <p>First Line <br>
               Second Line
            </p>
            <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
          </li>
          <li class="collection-item avatar">
            <img src="images/profile1.jpg" alt="" class="circle">
            <span class="title">Title</span>
            <p>First Line <br>
               Second Line
            </p>
            <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
          </li>
          <li class="collection-item avatar">
            <img src="images/profile2.jpg" alt="" class="circle">
            <span class="title">Title</span>
            <p>First Line <br>
               Second Line
            </p>
            <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
          </li>
          <li class="collection-item avatar">
            <img src="images/profile3.jpg" alt="" class="circle">
            <span class="title">Title</span>
            <p>First Line <br>
               Second Line
            </p>
            <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a>
          </li>
        </ul>
      </div>
      <hr></hr>
  </div>

</div>
